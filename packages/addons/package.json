{
  "name": "@joyboy-parser/addons",
  "version": "1.0.0",
  "type": "module",
  "description": "Extension system and addons for JoyBoy parser ecosystem",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "joyboy",
    "addons",
    "extensions",
    "plugins",
    "manga",
    "parser"
  ],
  "author": "alaric-senpai",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Alaric-senpai/The-joyboy-project.git",
    "directory": "packages/addons"
  },
  "homepage": "https://github.com/Alaric-senpai/The-joyboy-project#readme",
  "bugs": {
    "url": "https://github.com/Alaric-senpai/The-joyboy-project/issues"
  },
  "dependencies": {
    "@joyboy-parser/core": "^1.0.3",
    "@joyboy-parser/types": "^1.0.1"
  },
  "devDependencies": {
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vitest": "^1.6.1"
  },
  "peerDependencies": {
    "@joyboy-parser/core": "^1.0.3",
    "@joyboy-parser/types": "^1.0.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
