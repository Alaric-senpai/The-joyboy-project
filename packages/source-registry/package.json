{
  "name": "@joyboy-parser/source-registry",
  "version": "1.1.7",
  "type": "module",
  "description": "Dynamic source registry and catalog for JoyBoy parsers",
  "author": "JoyBoy Contributors",
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./sources.json": {
      "default": "./sources.json"
    }
  },
  "files": [
    "dist",
    "sources.json",
    "scripts"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist",
    "update-registry": "node scripts/update-registry.js",
    "gen-integrity": "node scripts/gen-integrity.js",
    "batch-integrity": "node scripts/batch-gen-integrity.js",
    "test": "vitest",
    "test:remote": "node test-remote-registry.js"
  },
  "keywords": [
    "joyboy",
    "manga",
    "registry",
    "catalog",
    "sources",
    "parser"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Alaric-senpai/The-joyboy-project.git",
    "directory": "packages/source-registry"
  },
  "homepage": "https://github.com/Alaric-senpai/The-joyboy-project/main/packages/source-registry#readme",
  "bugs": {
    "url": "https://github.com/Alaric-senpai/The-joyboy-project/issues"
  },
  "dependencies": {
    "@joyboy-parser/types": "workspace:*"
  },
  "devDependencies": {
    "tsup": "^8.0.1",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "peerDependencies": {
    "@joyboy-parser/types": "workspace:*"
  }
}