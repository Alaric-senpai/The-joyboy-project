{
  "name": "@joyboy-parser/source-mangafire",
  "version": "1.0.0",
  "type": "module",
  "description": "Mangafire source for joyboy",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "build:demo": "tsup src/demo.ts --format esm --target es2022 --no-dts",
    "dev": "tsup --watch",
    "test": "vitest",
    "clean": "rm -rf dist",
    "validate-meta": "node scripts/validate-meta.js",
    "demo": "pnpm build:demo && node dist/demo.js",
    "gen-hash": "pnpm build && shasum -a 256 dist/index.js",
    "gen-integrity": "node scripts/gen-integrity.js"
  },
  "keywords": [
    "joyboy",
    "manga",
    "mangafire",
    "parser"
  ],
  "license": "MIT",
  "dependencies": {
    "@joyboy-parser/addons": "^1.0.0",
    "@joyboy-parser/core": "^1.1.7",
    "@joyboy-parser/types": "^1.1.5"
  },
  "devDependencies": {
    "ajv": "^8.12.0",
    "ajv-formats": "^3.0.1",
    "tsup": "^8.0.1",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "peerDependencies": {
    "@joyboy-parser/core": "^1.1.7",
    "@joyboy-parser/types": "^1.1.5"
  }
}